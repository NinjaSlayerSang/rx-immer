{
  "version": "1.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rx-immer",
  "author": "xiaojunjiang",
  "module": "dist/rx-immer.esm.js",
  "size-limit": [
    {
      "path": "dist/rx-immer.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rx-immer.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/jsonpath-plus": "^5.0.2",
    "@types/lodash": "^4.14.186",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "immer": ">=9",
    "jsonpath-plus": ">=7",
    "lodash": ">=4",
    "lodash-es": ">=4",
    "rxjs": ">=7"
  }
}
